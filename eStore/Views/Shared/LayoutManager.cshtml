<!DOCTYPE html>
<html>
<head>
    <title>eStore - eCommerce - Bruno - Jefferson - Juliano</title>
    <link href="@Url.Content("~/Assets/Templates/GreTong/manager/")css/style.css" rel='stylesheet' type='text/css' />
    <script src="@Url.Content("~/Assets/Templates/GreTong/manager/")js/index.js"> </script>
    <link rel="icon" href="@Url.Content("~/Assets/Templates/GreTong/manager/")images/eStore_icone.png" sizes="32x32">
</head>
<body>
    <div class="header_bg">
        <div class="container">
            <div class="header">
                <div class="head-t">
                    <div style="text-align:right;margin:10px;float:right">
                        <a href="@Url.Action("Index","Home")" target="_blank"><img src="@Url.Content("~/Assets/Templates/GreTong/manager/")images/carrinho_minha_loja.png" class="img-responsive" alt="Minha Loja" /></a>
                        <a href="@Url.Action("Index","Home")"><img src="@Url.Content("~/Assets/Templates/GreTong/manager/")images/settings2.png" class="img-responsive" alt="Sair" /></a>
                    </div>
                    <div class="logo">
                        <a href="@Url.Action("LogIn","Manager")"><img src="@Url.Content("~/Assets/Templates/GreTong/manager/")images/eStorelogo2.png" class="img-responsive" alt="" /> </a>
                    </div>
                   

                    <div class="clearfix"> </div>
                </div>
                <!-- start header menu -->
               
                <nav id='cssmenu'>
                    
                    <div id="head-mobile"></div>
                    <div class="button"></div>
                    
                    <ul>
                        <li class=''>
                            <a href='#'>Pedidos</a>
                            <ul>
                                <li>
                                    <a href='#'>Gerenciar Pedidos</a>
                                </li>
                                <li>
                                    <a href='#'>Relatórios de Pedidos</a>
                                </li>
                               
                            </ul>
                        </li>
                        <li>
                            <a href='#'>Produtos</a>

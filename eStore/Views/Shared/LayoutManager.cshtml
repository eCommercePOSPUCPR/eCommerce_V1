                                    </ul>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a href='#'>Frete</a>
                            <ul>
                                <li>
                                    <a href="#">Gerenciar Frete</a>
                                    <ul>
                                        <li><a href="@Url.Action("Create", "Fretes")">Adicionar Frete</a></li>
                                        <li><a href="@Url.Action("Delete", "Fretes", new { id = 1})">Deletar Frete</a></li>

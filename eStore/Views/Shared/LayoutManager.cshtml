                                <li>
                                    <a href="@Url.Action("GerenciarProdutos","Manager")">Gerenciar Produtos</a>
                                </li>
                                <li>
                                    <a href='#'>Cadastrar Produtos</a>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a href='#'>Categorias</a>
                            <ul>
                                <li>
                                    <a href='#'>Gerenciar Categorias</a>
                                    <ul>
                                        <li><a href='#'>Sub Product</a></li>
                                        <li><a href='#'>Sub Product</a></li>
